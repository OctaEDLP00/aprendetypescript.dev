{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "isolatedModules": true,
    "jsx": "preserve",
    "module": "ESNext",
    "moduleResolution": "node",
    "outDir": "./dist",
    "paths": {
      "@/*": [ "src/*" ],
      "@/layouts/*": [ "src/layouts/*" ],
      "@/pages/*": [ "src/pages/*" ],
      "@/store/*": [ "src/store/*" ],
      "@/components/*": [ "src/components/*" ],
      "@/const/*": [ "src/const/*" ],
      "@/libs/*": [ "src/libs/*" ]
    },
    "resolveJsonModule": true,
    "target": "ESNext",
    "types": [ ],
  }
}
